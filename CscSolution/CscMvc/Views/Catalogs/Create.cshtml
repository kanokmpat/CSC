@model CscMvc.Models.tbSystemCatalog
@using PagedList;
@using PagedList.Mvc;



@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()


    <div class="form-horizontal">
    
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.ID)
        @Html.HiddenFor(model => model.CatalogID)
        @Html.HiddenFor(model => model.OrderSeq, new { htmlAttributes = new { @class = "form-control" } })
        @Html.HiddenFor(model => model.TypeID, new { htmlAttributes = new { @class = "form-control" } })
