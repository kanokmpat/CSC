                                    </div>
                                </div>

                            </div>

                            <!-- /.form-group -->
                        </div>

                        <div class="col-md-8">
                            <div class="form-group">
                                <label>ชื่อย่อ</label>

                                <div class="form-group">

                                    <div class="col-md-8">
                                        @Html.EditorFor(model => model.CatalogPrefix, new { htmlAttributes = new { @class = "form-control" } })
                                        @Html.ValidationMessageFor(model => model.CatalogPrefix, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                            </div>
                            <!-- /.form-group -->
                            <div class="form-group">
                              
                               
                            </div>
                            <!-- /.form-group -->
                        </div>


                        <div class="col-md-8">
                            <div class="form-group">
                                <label>รายละเอียด</label>

                                <div class="form-group">

                                    <div class="col-md-8">
                                        @Html.TextAreaFor(model => model.Remark, 10, 130, null)
                                        @Html.ValidationMessageFor(model => model.Remark, "", new { @class = "text-danger" })
                                    </div>
                                </div>

                            </div>
                            <!-- /.form-group -->
                            <div class="form-group">
                                <label></label>

                                <div class="form-group">

                                    <div class="col-md-10">
                                       
                                    </div>
                                </div>

                            </div>
                            <!-- /.form-group -->
                        </div>

                        <!-- /.col -->


                    </div>
                    <!-- /.row -->
                </div>

                <div class="">
